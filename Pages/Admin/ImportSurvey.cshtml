@page
@model ImportSurveyModel
@{
    Layout = "_Layout";
    ViewData["Title"] = "Import Survey";
}

<form enctype="multipart/form-data" method="post">
    <ul>
        <li>
            <label asp-for="Upload!.SurveyFile"></label>
            <input asp-for="Upload!.SurveyFile" type="file" />
        </li>
        <li class="button">
            <button type="submit" asp-page-handler="upload">Upload</button>
        </li>
        <hr />
        <li>
            <label asp-for="Download!.SurveyFile"></label>
            <input asp-for="Download!.SurveyFile" type="url" />
        </li>
        <li class="button">
            <button type="submit" asp-page-handler="download">Download</button>
        </li>
    </ul>
</form>